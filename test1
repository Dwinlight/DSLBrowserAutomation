###Test1###
GOTO "http://www.imt-atlantique.fr/fr" ON firefox 
FIND link WHERE  content CONTAINS "Toutes les actualités"

###Test2###
GOTO "http://www.imt-atlantique.fr/fr" ON firefox 
CLICK link WHERE content CONTAINS "Toutes les actualités" FIND  link WHERE content  CONTAINS "Accueil"

###Test3###
GOTO "http://www.imt-atlantique.fr/fr" ON firefox
CLICK link WHERE  content CONTAINS "Toutes les actualités" 
CLICK image WHERE  alt EQUALS "Accueil"
FIND link WHERE content CONTAINS "Accueil" 

###Test4###
GOTO "http://www.imt-atlantique.fr/fr/rechercher" ON firefox
SET  value "Donald Trump" WHERE  id EQUALS "edit-search-api-fulltext"
CLICK button WHERE value CONTAINS "Appliquer les filtres"
FIND  content CONTAINS "Aucun résultat ne correspond à votre recherche"

###Test5###
GOTO "http://www.imt-atlantique.fr/fr/formation/trouver-ma-formation" ON firefox
SET  checked false WHERE  type EQUALS "checkbox" 
SET  checked true WHERE  Content CONTAINS "Temps plein" //GET retourne le 1er respectant la condition
SET  checked true  WHERE Content CONTAINS "A domicile"
SET checked true WHERE  Content CONTAINS "Anglais"
CLICK button WHERE value CONTAINS "Appliquer les critères"
FIND Body CONTENT "No found course matching your criteria."

###Test6###
 GOTO "http://www.imt-atlantique.fr/fr/rechercher" ON firefox
SET  value "2007" WHERE  id EQUALS "edit-search-api-fulltext"
CLICK li WHERE  content EQUALS "Le mois dernier"
CLICK button WHERE value CONTAINS "Appliquer les critères"
FIND  content CONTAINS "Aucun résultat ne correspond à votre recherche"

###Test7###
GOTO "http://www.imt-atlantique.fr/fr" ON firefox
$titre = GET 1  content WHERE  class EQUALS "actu_home_ctner_inner_cell1_titre"
CLICK link WHERE  title EQUALS $titre
FIND  content CONTAINS $titre

###Test8###
GOTO "http://www.imt-atlantique.fr/fr" ON firefox
$titre = GET 1  content WHERE  class EQUALS "actu_home_ctner_inner_cell1_titre"
$url =  GET link WHERE  title EQUALS $titre

GOTO "http://www.imt-atlantique.fr/fr/rechercher" ON firefox
SET  value $titre" WHERE  id EQUALS "edit-search-api-fulltext"
CLICK button WHERE value CONTAINS "Appliquer les critères"
FIND  link CONTAINS "Aucun résultat ne correspond à votre recherche"


